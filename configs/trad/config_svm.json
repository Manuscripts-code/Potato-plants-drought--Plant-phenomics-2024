{
    "name": "SVC",

    "model": {
        "type": "Model",
        "args": {
            "pipeline": ["SVC"],
            "unions": {
            }
        }
    },
    "data_loader": {
        "type": "HypDataLoader",
        "args": {
            "dataset": "PlantsDataset",
            "data_sampler": "ManualGroupSamplerKrka",
            "batch_size": 32,
            "shuffle": true,
            "train_test_split_size": 0.2,
            "train_valid_split_size": 0.2,
            "num_workers": 4
        }
    },
    "tuned_parameters":{
                        "SVC__kernel": ["CHOICE", "rbf"],
                        "SVC__gamma": ["LOG_UNIFORM", 1e-4, 1],
                        "SVC__C": ["LOG_UNIFORM", 1, 1000]
                    },

    "optimizer": "OptimizerClassification",
    "validation": {
        "type": "RepeatedStratifiedKFold",
        "args": {
            "n_splits": 5,
            "n_repeats": 3,
            "random_state": 1
        }
    },

    "num_samples": 200,
    "scoring_metric": "f1",
    "scoring_mode": "max",
    "debug": false
}
